{
  "log": {
    "level": "TRACE"
  },
  "auth": {
    "enforced": true,
    "jwt": {
      "secret": "{{JWT_SECRET}}"
    }
  },
  "database": {
    "url": "{{DATABASE_URL}}",
    "max_connections": 10,
    "min_connections": 5,
    "max_connection_lifetime_minutes": 60,
    "max_connection_idle_time_minutes": 30
  }
}